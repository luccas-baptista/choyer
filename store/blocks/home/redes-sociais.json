{
  "flex-layout.row#redes-sociais": {
    "children": ["flex-layout.col#pinterest", "flex-layout.col#youtube", "flex-layout.col#twitter", "flex-layout.col#tiktok", "flex-layout.col#insta", "flex-layout.col#facebook" ],
    "props": {
      "textPosition": "center",
        "preventHorizontalStretch": true,
        "preventVerticalStretch": true,
        "paddingTop": 0,
        "paddingBottom": 4,
        "blockClass": "social-elevage"
    }
  },

  "flex-layout.col#pinterest": {
    "children": [
      "image#pinterest"
    ],
    "props": {
      "blockClass": "box-icone-social"
    }
  },

  "image#pinterest": {
    "props": {
      "src": "assets/imgs/footer/pinterest-choyer.svg",
      "textAlignment": "center",
      "blockClass": "img-redes-sociais",
      "link": {
        "url":"https://br.pinterest.com/choyerprofessional/",
        "newTab": true
      }
    }

  },

